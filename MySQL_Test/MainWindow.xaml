<Window x:Class="MySQL_Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MySQL_Test"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="280*"/>
            <RowDefinition Height="39*"/>
        </Grid.RowDefinitions>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="22,10,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <StackPanel HorizontalAlignment="Left" Height="100" Margin="97,118,0,0" VerticalAlignment="Top" Width="348">
            <TextBlock x:Name="textBlock" TextWrapping="Wrap" Text="TextBlock"/>
            <TextBlock x:Name="textBlock1" TextWrapping="Wrap" Text="TextBlock"/>
            <TextBox x:Name="textBox" Height="23" TextWrapping="Wrap" Text="TextBox"/>
        </StackPanel>

    </Grid>
</Window>
